<h1>Resigter</h1>
<%= form_with model: @player do |f| %> 
    <%= f.label :username %> 
    <%= f.text_field :username %>
    <br>
    <%= f.label :password %> 
    <%= f.text_field :password %>
    <br>
    <%= f.label :password_confirmation %> 
    <%= f.text_field :password_confirmation %>
    <br>
    <%= f.submit "Resigter" %> 
<% end %> 